{
    "manifest_version": 3,
    "name": "Date Extractor",
    "version": "1.0",
    "description": "Extracts the exact date from ambiguously dated posts",
    "permissions": [
        "activeTab",
        "scripting"

    ],
    "content_scripts": [
        {
            "matches": ["https://www.google.com/maps/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
      }
    }